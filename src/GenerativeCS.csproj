      completion-generator completion-provider conversation conversational conversational-ai cs
      csharp davinci dialog dotnet dotnet-core embedding embedding-model embeddings embeddings-model
      function function-calling functional functional-gpt functions functions-calling gemini
      gemini-api gemini-api-client gemini-client gemini-pro gemini-pro-api gemini-pro-api-client
      gemini-pro-client generation generative-ai generative-cs generator google google-gemini
      google-gemini-pro google-gemini-pro-api google-gemini-pro-api-client google-gemini-pro-client
      gpt gpt-3 gpt-4 gpt-function gpt-functions gpt3 gpt4 language language-model learning library
      llama llm machine machine-learning method method-calling methods methods-calling ml model
      moderation natural natural-language-processing nlp open-ai open-ai-api open-ai-client openai
      openai-api openai-api-client openai-client pro processing prompt provider reflection response
      response-completion response-generation sound speech speech-to-text streaming synthesis text
      text-completion text-embedding text-embeddings text-generation text-synthesis text-to-speech
      token transcript transcription transformer transformer-model transformers transformers-model
      translation translator tts turbo virtual virtual-assistant voice whisper whisper-api wrapper
    </PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <None
        Include="icon.png"
        Pack="true"
        PackagePath="\" />
