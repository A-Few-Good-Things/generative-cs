        Include="Microsoft.Extensions.DependencyInjection"
        Version="8.0.0" />
    <PackageReference
        Include="Microsoft.Extensions.Options"
        Version="8.0.1" />
    <PackageReference
        Include="Microsoft.Extensions.Http"
        Version="8.0.0" />
  </ItemGroup>
</Project>